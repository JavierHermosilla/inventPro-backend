{
  "name": "inventpro",
  "version": "1.0.0",
  "description": "Sistema de gestión de inventarios y órdenes de compra con panel de administrador para pymes",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JavierHermosilla/inventpro.git"
  },
  "keywords": [
    "inventario",
    "ordenes",
    "compra",
    "sistema",
    "express",
    "nodejs",
    "postgresql",
    "sequelize",
    "api-rest",
    "gestión",
    "pymes",
    "panel-administrativo",
    "jwt"
  ],
  "author": "Javier Hermosilla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JavierHermosilla/inventpro/issues"
  },
  "homepage": "https://github.com/JavierHermosilla/inventpro#readme",
  "dependencies": {
    "bcryptjs": "3.0.2",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "17.2.0",
    "express": "5.1.0",
    "express-rate-limit": "8.0.1",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "morgan": "1.10.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "winston": "3.17.0",
    "xss": "1.0.15",
    "xss-clean": "0.1.4",
    "zod": "4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.0.5",
    "jest": "^30.0.5",
    "nodemon": "3.1.10",
    "standard": "17.1.2",
    "supertest": "7.1.4"
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
